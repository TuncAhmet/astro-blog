---
---

<footer class="bottom-0 absolute w-full flex border justify-around p-4">
  <div class="logo">
    <a href="https://www.{{ site.domain }}" title="{{ site.title }}">
      <!-- <img
        src="{{ site.baseurl }}/assets/images/logo.png"
        alt="{{ site.title }}"
      /> -->
      Astro Blog
    </a>
  </div>
  <div class="links flex flex-col">
    <h3 class="text-black font-bold">Links</h3>
    <ul class="flex flex-col">
      <li class="my-2">
        <a href="https://www.{{ site.domain }}/about">About</a>
      </li>
      <li class="my-2">
        <a href="https://www.{{ site.domain }}/contact">Contact</a>
      </li>
      <li class="my-2">
        <a href="https://www.{{ site.domain }}/privacy">Privacy</a>
      </li>
      <li class="my-2">
        <a href="https://www.{{ site.domain }}/terms">Terms</a>
      </li>
    </ul>
  </div>
  <div class="socials flex flex-col">
    <h3 class="text-black font-bold">Social Links</h3>
    <ul class="flex flex-col">
      <li class="my-2">
        <a
          href="https://www.facebook.com/{{ site.facebook }}"
          target="_blank"
          title="Facebook"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            ><path
              fill="#2563eb"
              d="M22 12c0-5.52-4.48-10-10-10S2 6.48 2 12c0 4.84 3.44 8.87 8 9.8V15H8v-3h2V9.5C10 7.57 11.57 6 13.5 6H16v3h-2c-.55 0-1 .45-1 1v2h3v3h-3v6.95c5.05-.5 9-4.76 9-9.95z"
            ></path></svg
          ></a
        >
      </li>
      <li class="my-2">
        <a
          href="https://www.twitter.com/{{ site.twitter }}"
          target="_blank"
          title="Twitter"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            ><path
              fill="#2563eb"
              d="M22.46 6c-.77.35-1.6.58-2.46.69c.88-.53 1.56-1.37 1.88-2.38c-.83.5-1.75.85-2.72 1.05C18.37 4.5 17.26 4 16 4c-2.35 0-4.27 1.92-4.27 4.29c0 .34.04.67.11.98C8.28 9.09 5.11 7.38 3 4.79c-.37.63-.58 1.37-.58 2.15c0 1.49.75 2.81 1.91 3.56c-.71 0-1.37-.2-1.95-.5v.03c0 2.08 1.48 3.82 3.44 4.21a4.22 4.22 0 0 1-1.93.07a4.28 4.28 0 0 0 4 2.98a8.521 8.521 0 0 1-5.33 1.84c-.34 0-.68-.02-1.02-.06C3.44 20.29 5.7 21 8.12 21C16 21 20.33 14.46 20.33 8.79c0-.19 0-.37-.01-.56c.84-.6 1.56-1.36 2.14-2.23Z"
            ></path></svg
          ></a
        >
      </li>
      <li class="my-2">
        <a
          href="https://www.instagram.com/{{ site.instagram }}"
          target="_blank"
          title="Instagram"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            ><path
              fill="#2563eb"
              d="M7.8 2h8.4C19.4 2 22 4.6 22 7.8v8.4a5.8 5.8 0 0 1-5.8 5.8H7.8C4.6 22 2 19.4 2 16.2V7.8A5.8 5.8 0 0 1 7.8 2m-.2 2A3.6 3.6 0 0 0 4 7.6v8.8C4 18.39 5.61 20 7.6 20h8.8a3.6 3.6 0 0 0 3.6-3.6V7.6C20 5.61 18.39 4 16.4 4H7.6m9.65 1.5a1.25 1.25 0 0 1 1.25 1.25A1.25 1.25 0 0 1 17.25 8A1.25 1.25 0 0 1 16 6.75a1.25 1.25 0 0 1 1.25-1.25M12 7a5 5 0 0 1 5 5a5 5 0 0 1-5 5a5 5 0 0 1-5-5a5 5 0 0 1 5-5m0 2a3 3 0 0 0-3 3a3 3 0 0 0 3 3a3 3 0 0 0 3-3a3 3 0 0 0-3-3Z"
            ></path></svg
          ></a
        >
      </li>
      <li class="my-2">
        <a
          href="https://www.youtube.com/{{ site.youtube }}"
          target="_blank"
          title="YouTube"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            ><path
              fill="#2563eb"
              d="m10 15l5.19-3L10 9v6m11.56-7.83c.13.47.22 1.1.28 1.9c.07.8.1 1.49.1 2.09L22 12c0 2.19-.16 3.8-.44 4.83c-.25.9-.83 1.48-1.73 1.73c-.47.13-1.33.22-2.65.28c-1.3.07-2.49.1-3.59.1L12 19c-4.19 0-6.8-.16-7.83-.44c-.9-.25-1.48-.83-1.73-1.73c-.13-.47-.22-1.1-.28-1.9c-.07-.8-.1-1.49-.1-2.09L2 12c0-2.19.16-3.8.44-4.83c.25-.9.83-1.48 1.73-1.73c.47-.13 1.33-.22 2.65-.28c1.3-.07 2.49-.1 3.59-.1L12 5c4.19 0 6.8.16 7.83.44c.9.25 1.48.83 1.73 1.73Z"
            ></path></svg
